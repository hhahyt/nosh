<ParameterList>

  <ParameterList name="Continuation Manager">

    <ParameterList name="Input/Output">
      <!--Parameter name="Initial Guess File" type="string" value="./initialGuess_Next_To_Pitchfork.vtk"/-->
      <!--<Parameter name="Initial Guess File" type="string" value="./initialGuess_Bottom_Of_The_Snake_Reflected.vtk"/>-->
      <Parameter name="Initial Guess File" type="string" value="./initialGuess.vtk"/>
      <!--Parameter name="Initial Guess File" type="string" value="./initialGuess_Branch_Switched.vtk"/>-->

      <ParameterList name="Solution File Parameters">
	<Parameter name="Nx" type="int" value="50"/>
	<Parameter name="p" type="double" value="0.0"/>
	<Parameter name="s" type="double" value="0.0"/>
	<Parameter name="lx" type="double" value="0.0"/>
	<Parameter name="ly" type="double" value="0.0"/>
	<Parameter name="c" type="double" value="0.0"/>
      </ParameterList>

    </ParameterList>

    <ParameterList name="Continuation">
      <Parameter name="Steps Per Print" type="int" value="1"/>
      <Parameter name="Label From Step" type="int" value="0"/>
      <!--Parameter name="Nonlinear Step Tolerance" type="double" value="5.0e-5"/-->
      <Parameter name="Nonlinear Step Tolerance" type="double" value="1.0e-8"/>
    </ParameterList>

    <ParameterList name="Stability">
      <Parameter name="Compute Stability" type="bool" value="false"/>
      <Parameter name="Print Eigenfunctions" type="bool" value="false"/>
    </ParameterList>

    <ParameterList name="Constraint">
      <Parameter name="Bordered Solver Method" type="string" value="Bordering"/>
      <ParameterList name="Constraint Parameters">
	<Parameter name="constrained parameter 1" type="string" value="c"/>
      </ParameterList>

    </ParameterList>

    <ParameterList name="Perturbation">
      <Parameter name="Active" type="bool" value="false"/>
      <Parameter name="Epsilon" type="double" value="0.001"/>
    </ParameterList>

  </ParameterList>

  <ParameterList name="NOX and LOCA"> 

    <ParameterList name="LOCA">

      <ParameterList name="Predictor">
	<Parameter name="Method" type="string" value="Secant"/>
        <!--Parameter name="Method" type="string" value="Tangent"/-->
        <!--Parameter name="Method" type="string" value="Constant"/-->
      </ParameterList>

      <ParameterList name="Step Size">
	<Parameter name="Failed Step Reduction Factor" type="double" value="0.01"/>
	<!--Parameter name="Initial Step Size" type="double" value="0.0001"/-->
        <Parameter name="Initial Step Size" type="double" value="0.01"/>
	<!--Parameter name="Max Step Size" type="double" value="0.0005"/-->
        <Parameter name="Max Step Size" type="double" value="10.0"/>
      </ParameterList>

      <ParameterList name="Stepper">
        <!--Parameter name="Continuation Method" type="string" value="Natural"/-->
	<Parameter name="Bordered Solver Method" type="string" value="Nested"/>
        <Parameter name="Enable Arc Length Scaling" type="bool" value="1"/>
	<Parameter name="Continuation Parameter" type="string" value="p"/>
	<!--Parameter name="Initial Value" type="double" value="0.638561"/-->
        <Parameter name="Initial Value" type="double" value="6.135340e-01"/>
	<Parameter name="Max Nonlinear Iterations" type="int" value="4"/>
	<Parameter name="Max Steps" type="int" value="1000"/>
	<Parameter name="Max Value" type="double" value="5"/>
	<Parameter name="Min Value" type="double" value="0"/>

	<ParameterList name="Nested Bordered Solver">
	  <Parameter name="Bordered Solver Method" type="string" value="Householder"/>
	  <Parameter name="Include UV In Preconditioner" type="bool" value="true"/>
	</ParameterList>

        <Parameter name="Compute Eigenvalues" type="bool" value="0"/>
        <ParameterList name="Eigensolver">
          <Parameter name="Block Size" type="int" value="1"/>
          <Parameter name="Convergence Tolerance" type="double" value="1e-9"/>
          <Parameter name="Maximum Restarts" type="int" value="0"/>
          <Parameter name="Method" type="string" value="Anasazi"/>
          <Parameter name="Num Blocks" type="int" value="100"/>
          <Parameter name="Num Eigenvalues" type="int" value="10"/>
          <Parameter name="Step Size" type="int" value="1"/>
          <Parameter name="Verbosity" type="int" value="9"/>
        </ParameterList>


      </ParameterList>

    </ParameterList>

    <ParameterList name="NOX">

      <ParameterList name="Direction">
	<Parameter name="Method" type="string" value="Newton"/>

	<ParameterList name="Newton">

	  <!--Parameter name="Forcing Term Method" type="string" value="Type 1" /-->
	  <!--Parameter name="Forcing Term Maximum Tolerance" type="double" value="1.0e-3" /-->

	  <ParameterList name="Linear Solver">
            <Parameter name="Aztec Solver" type="string" value="GMRES" />
	    <Parameter name="Max Iterations" type="int" value="800" />
	    <Parameter name="Output Frequency" type="int" value="50" />
	    <Parameter name="Scaling" type="string" value="None" />
	    <Parameter name="Size of Krylov Subspace" type="int" value="800" />
	    <Parameter name="Tolerance" type="double" value="1e-9" />
	    <Parameter name="Preconditioner" type="string" value="ML" />

	    <ParameterList name="ML">
	      <Parameter name="default values" type="string" value="SA" />
	      <Parameter name="smoother: sweeps" type="int" value="2" />
	      <Parameter name="smoother: damping factor" type="double" value="1.7" />
	      <Parameter name="smoother: type" type="string" value="symmetric Gauss-Seidel" />
	      <Parameter name="aggregation: damping factor" type="double" value="2.2" />
	      <Parameter name="aggregation: smoothing sweeps" type="int" value="2" />
	      <Parameter name="coarse: max size" type='int' value="70000" />
	    </ParameterList>

	  </ParameterList>

        </ParameterList>

        <Parameter name="Rescue Bad Newton Solve" type="bool" value="1" />
      </ParameterList>

      <!--Parameter name="Nonlinear Solver" type="string" value="Trust Region Based"/-->

      <ParameterList name="Printing">
	<Parameter name="MyPID" type="int" value="0"/>
	<Parameter name="Output Information" type="int" value="6143"/>
	<Parameter name="Output Precision" type="int" value="6"/>
	<Parameter name="Output Processor" type="int" value="0"/>
      </ParameterList>

    </ParameterList>

  </ParameterList>

</ParameterList>